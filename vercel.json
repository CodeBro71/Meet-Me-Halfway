{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/meet-me-half-way/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/meet-me-half-way/public/index.html"
    }
  ]
}